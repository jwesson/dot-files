{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "palette": {
    "my-background": "white",
    "my-foreground": "blue",
    "my-success": "cyan",
    "my-error": "yellow"
  },
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "background": "p:my-background",
          "foreground": "p:my-foreground",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "template": " {{ .UserName }}@{{ .HostName }} ",
          "type": "session"
        },
        {
          "background": "p:my-background",
          "foreground": "p:my-foreground",
          "powerline_symbol": "\ue0b0",
          "properties": {
            "folder_icon": "\uf115",
            "folder_separator_icon": " \ue0b1 ",
            "home_icon": "\uf7db",
            "style": "agnoster"
          },
          "style": "powerline",
          "template": " {{ .Path }} ",
          "type": "path"
        },
        {
          "background": "p:my-background",
          "foreground": "p:my-foreground",
          "properties": {
            "cherry_pick_icon": "\u2713 ",
            "commit_icon": "\u25b7 ",
            "fetch_status": true,
            "merge_icon": "\u25f4 ",
            "no_commits_icon": "[no commits]",
            "rebase_icon": "\u2c62 ",
            "tag_icon": "\u25b6 "
          },
          "style": "diamond",
          "template": " {{ .HEAD }}{{ if and (eq .Ahead 0) (eq .Behind 0) }} \u2261{{end}}{{ if gt .Ahead 0 }} \u2191{{.Ahead}}{{end}}{{ if gt .Behind 0 }} \u2193{{.Behind}}{{end}} {{ if .Working.Changed }}+{{ .Working.Added }} ~{{ .Working.Modified }} -{{ .Working.Deleted }} {{ end }}",
          "type": "git",
          "trailing_diamond": "\ue0b0",
          "leading_diamond": "<transparent,p:my-background>\ue0b0</>"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "background": "p:my-success",
          "foreground": "p:my-foreground",
          "style": "diamond",
          "leading_diamond": "\ue0b6",
          "trailing_diamond": "\ue0b0",
          "type": "exit",
          "template": "{{ if gt .Code 0 }} \uf00d Error: {{.Code}} {{ else }} \uf42e {{ end }}",
          "background_templates": [
            "{{ if gt .Code 0 }}p:my-error{{ end }}"
          ],
          "properties": {
            "always_enabled": true
          }
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}